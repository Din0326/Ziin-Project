# Discord
DISCORD_TOKEN=your_token_here
DISCORD_PREFIX=z!

# Optional: set to 1 to sync slash commands on startup
SYNC_COMMANDS=0

# Shared DB path (recommended)
# LOCAL_DB_PATH=d:/din/ziin-project/data/local.db

# Twitch / YouTube APIs
TWITCH_CLIENT_ID=
TWITCH_CLIENT_SECRET=
YOUTUBE_API_KEY=


# Error reporting (optional)
# Channel to receive automatic error reports
ERROR_REPORT_CHANNEL_ID=
# Show IDs (guild/channel/user) in debug text
ERROR_REPORT_SHOW_IDS=1
# Error messages sent to users as ephemeral (1) or normal messages (0)
ERROR_REPORT_EPHEMERAL=1
